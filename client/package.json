  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.19.0",
    "firebase": "^6.1.0",
    "react": "^16.8.6",
    "react-async": "^6.2.0",
    "react-dom": "^16.8.6",
    "react-firebase-hooks": "^2.0.0",
    "react-scripts": "3.0.1",
    "react-spotify-web-playback": "^0.4.0",
